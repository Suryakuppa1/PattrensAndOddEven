import java.util.Scanner;
class  reverseChar
{
	public static void main(String[] args) 
	{
		Scanner scan = new Scanner(System.in);
		System.out.println("Enter the String:");
		String s= scan.nextLine();
		for (int i=s.length()-1;i>=0 ;i--)
		{
		 s=""+s.charAt(i);
		}
		System.out.println(s);
	}
}
