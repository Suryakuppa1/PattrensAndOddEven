import java.util.*;
class  prime
{
	public static void main(String[] args) 
	{
		Scanner scan = new Scanner (System.in);
		System.out.println("Enter the Number");
		int n =scan.nextInt();
		int count = 0;
		for(int i=2; i<=n/2; i++)
		{
			if(i%2==0)
			{
				count++;
			}
			
		}
		System.out.println(count);
			


	}
}
